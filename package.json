{
  "name": "react-video-gallery",
  "version": "1.0.0",
  "description": "Jahia module that provides a React Video Gallery",
  "main": "main.js",
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "scripts": {
    "build": "yarn lint --fix && webpack",
    "build:nolint": "webpack",
    "build:analyze": "yarn build --analyze",
    "build:production": "yarn build --mode=production",
    "build:production-analyze": "yarn build:production --analyze",
    "dev": "yarn webpack --watch",
    "clean": "rimraf *.log src/main/resources/javascript/app",
    "clean:all": "yarn clean && rimraf node_modules node",
    "test": "jest",
    "lint": "eslint --ext js,jsx .",
    "lint:fix": "eslint --ext js,jsx --fix .",
    "version": "npm version `auto version` -m 'Bump version to: %s [skip ci]'",
    "publish": "npm publish && git push --follow-tags --set-upstream origin $branch",
    "release": "./scripts/release.sh"
  },
  "author": "Stephane Monier for Jahia",
  "repository": "git+https://github.com/Jahia/react-video-gallery.git",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^3.11.2",
    "@cyclonedx/webpack-plugin": "^4.0.1",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@google/generative-ai": "^0.21.0",
    "@jahia/webpack-config": "^1.1.0",
    "@mui/icons-material": "^6.4.3",
    "@mui/material": "^6.4.3",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "apache-unomi-tracker": "^1.4.0",
    "buffer": "^6.0.3",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "clsx": "^2.1.1",
    "dompurify": "^3.1.6",
    "graphql": "^16.9.0",
    "i18next": "^23.12.2",
    "lodash.merge": "^4.6.2",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.0",
    "react-icons": "^5.2.1",
    "react-markdown": "^9.0.3",
    "react-player": "^2.16.0",
    "sass": "^1.77.8",
    "sass-loader": "^16.0.0"
  },
  "devDependencies": {
    "@babel/compat-data": "^7.25.2",
    "@babel/core": "^7.25.2",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "^7.24.7",
    "@jahia/eslint-config": "^2.1.2",
    "@jahia/moonstone": "^2.11.3",
    "auto": "^11.1.2",
    "babel-eslint": "10.1.0",
    "babel-jest": "^25.3.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-lodash": "^3.3.4",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "cryptiles": ">=4.1.2",
    "css-loader": "^5.2.7",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "file-loader": "^6.2.0",
    "hoek": "^6.1.3",
    "husky": "^9.1.4",
    "pom-parser": "^1.2.0",
    "rimraf": "^5.0.5",
    "style-loader": "^2.0.0",
    "webpack": "^5.91.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "engines": {
    "yarn": ">=1.22.22"
  }
}
